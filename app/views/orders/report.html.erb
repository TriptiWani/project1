<div class="prod_home col-md-6 col-md-offset-3 col-xs-6 col-xs-offset-3">
    <div class="panel panel-default">
        <div class="panel-heading ">Reports</div>
        <form action="/orders" method="get">
          <fieldset class="col-md-4 col-md-offset-2 col-xs-5 col-xs-offset-1">
            <label class="gender_label" for="from">From</label>
            <input type="date" name="from_date">
          </fieldset>
          <fieldset class="col-md-4 col-md-offset-1 col-xs-5 col-xs-offset-1">
            <label class="gender_label" for="to">To</label>
            <input type="date" name="to_date">
          </fieldset>
          <button class="col-md-4 col-md-offset-4 col-xs-4  col-xs-offset-4">Generate Report</button>
        </form>
      </div>
    </div>
  </div>
