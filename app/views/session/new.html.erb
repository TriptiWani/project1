<h1 class="prod_home">Login</h1>
<div class="col-xs-6 col-md-6 col-md-offset-3">
    <%= form_tag '/login' do %>
    <div>
      <%= label_tag :email,nil,:autofocus => true %>
      <%= text_field_tag :email%>
    </div>

    <%= label_tag :password %>
    <%= password_field_tag :password  %>
    <%= submit_tag 'Login',:class=>  'col-xs-offset-6'%>
    <% end %>
</div>
