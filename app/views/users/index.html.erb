<div class="prod_home">
  <div class="panel panel-default table_display">
    <div class="panel-heading">Users List</div>
    <table class="table">
        <% @users.each do |user| %>
        <tr>
            <td>
                <%= link_to user.first_name+' '+user.last_name , user %>
            </td>
            <td>
                <%= ((user.active)?'Active':'Inactive') %>
            </td>
        </tr>
        <% end %>
    </table>
  </div>
</div>
